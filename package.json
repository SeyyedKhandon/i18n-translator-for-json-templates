  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SeyyedKhandon/i18n-translator-for-json-templates/issues"
  },
  "homepage": "https://github.com/SeyyedKhandon/i18n-translator-for-json-templates#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.4",
    "@types/ramda": "^0.27.4",
    "eslint": "^7.0.0",
    "eslint-plugin-lodash-fp": "^2.2.0-a1",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "ts-jest": "^25.4.0",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "ramda": "^0.27.0"
  }
